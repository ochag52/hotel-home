{
  "name": "hotel-home",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy/local.json",
    "start:prod": "ng serve --proxy-config proxy/prod.json",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run hotel-home:serve-ssr",
    "serve:ssr": "node dist/hotel-home/server/main.js",
    "build:ssr": "ng build && ng run hotel-home:server",
    "prerender": "ng run hotel-home:prerender",
    "artifact": "rm artifact.zip && cd dist/hotel-home/browser/ && zip -r artifact.zip ./ && mv artifact.zip ../../../"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.0",
    "@angular/cdk": "^12.1.0",
    "@angular/common": "~12.0.0",
    "@angular/compiler": "~12.0.0",
    "@angular/core": "~12.0.0",
    "@angular/forms": "~12.0.0",
    "@angular/platform-browser": "~12.0.0",
    "@angular/platform-browser-dynamic": "~12.0.0",
    "@angular/platform-server": "~12.0.0",
    "@angular/router": "~12.0.0",
    "@mdi/font": "^5.9.55",
    "@nguniversal/express-engine": "^12.0.0",
    "animate.css": "^4.1.1",
    "bootstrap": "^5.0.1",
    "express": "^4.15.2",
    "ng-gallery": "^5.0.0",
    "normalize.css": "^8.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.0",
    "@angular/cli": "~12.0.0",
    "@angular/compiler-cli": "~12.0.0",
    "@nguniversal/builders": "^12.0.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "typescript": "~4.2.3"
  }
}
